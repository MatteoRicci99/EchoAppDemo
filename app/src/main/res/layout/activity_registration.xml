            app:layout_constraintHorizontal_bias="0.076" app:layout_constraintVertical_bias="0.449"
            android:autofillHints=""/>
    <EditText
            android:layout_width="150dp"
            android:layout_height="45dp"
            android:inputType="number"
            android:ems="10"
            android:id="@+id/nUtenze"
            android:hint="@string/n_utenze_totali"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.923" app:layout_constraintVertical_bias="0.358"
            android:autofillHints=""/>
</androidx.constraintlayout.widget.ConstraintLayout>